ring {\n    return string.substring(0, 1).toUpperCase() + string.substring(1).toLowerCase();\n  }\n\n  //takes the index(+1) of the clicked link and creates a valid routing string based on how 'deep' the link is\n  //in case the second link gets clicked, it will have to take the first and second part of the location array.\n  navigate(index: number) {\n    let location = \"\";\n    for (let i = 0; i < index; i++) {\n      location += \"/\" + this.currentRoute[i];\n    }\n    this.router.navigate([location.toLowerCase()]);\n  }\n\n  logOut() {\n    this.authentication.signOut();\n    // this.meldingService.ngOnDestroy();\n    this.router.navigate(['/signin']);\n  }\n}\n","export default \"\\n/*# sourceMappingURL=data:application/json;base64,eyJ2ZXJzaW9uIjozLCJzb3VyY2VzIjpbXSwibmFtZXMiOltdLCJtYXBwaW5ncyI6IiIsImZpbGUiOiJzcmMvYXBwL2NvbXBvbmVudHMvcGFnZS1ub3QtZm91bmQvcGFnZS1ub3QtZm91bmQuY29tcG9uZW50LmNzcyJ9 */\"","import { Component, OnInit } from '@angular/core';\n\n@Component({\n  selector: 'app-page-not-found',\n  templateUrl: './page-not-found.component.html',\n  styleUrls: ['./page-not-found.component.css']\n})\nexport class PageNotFoundComponent implements OnInit {\n\n  constructor() { }\n\n  ngOnInit() {\n  }\n\n}\n","export default \"form { max-width:420px; margin:20px auto 0; }\\nh3 {text-align: center}\\n::ng-deep .feedback-input {\\n  font-weight:500;\\n  font-size: 18px;\\n  border-radius: 5px;\\n  line-height: 22px;\\n  border:2px solid #00AAE7;\\n  -webkit-transition: all 0.3s;\\n  transition: all 0.3s;\\n  padding: 13px;\\n  margin-bottom: 15px;\\n  width:100%;\\n  box-sizing: border-box;\\n  background-color: white;\\n}\\n::ng-deep .dropdown-input {\\n  font-weight:500;\\n  font-size: 18px;\\n  border-radius: 5px 5px 0 0;\\n  line-height: 22px;\\n  border:2px solid #00AAE7;\\n  -webkit-transition: all 0.3s;\\n  transition: all 0.3s;\\n  padding: 13px;\\n  width:100%;\\n  box-sizing: border-box;\\n}\\n::ng-deep input.ng-touched.ng-valid {\\n  border:2px sol