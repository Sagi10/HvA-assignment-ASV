;\n  }\n\n  getMainTires(): number {\n    if (this.mainTires === undefined) {\n      return 0;\n    }\n    return this.mainTires;\n  }\n\n  getTireAmount(): number {\n    if (this.noseTires === undefined && this.mainTires === undefined) {\n      return 0\n    } else if (this.noseTires === undefined) {\n      return this.mainTires;\n    } else if (this.mainTires === undefined) {\n      return this.noseTires;\n    } else {\n      return this.noseTires + this.mainTires;\n    }\n  }\n\n}\n","export default \"h1 {\\n  text-align: center;\\n}\\n\\ntable{\\n  border-collapse: collapse;\\n}\\n\\nth, td{\\n  padding: 8px;\\n  font-size: var(--application-font-size);\\n}\\n\\n.table thead th {\\n  border-bottom: none;\\n}\\n\\nth {\\n  cursor: default;\\n  text-align: center;\\n  color: white;\\n  background-color: #00AAE7;\\n  border: none;\\n}\\n\\ntd {\\n  cursor: pointer;\\n  overflow: hidden;\\n  white-space: nowrap;\\n  text-overflow: ellipsis;\\n  max-width: 280px;\\n  text-align: center;\\n}\\n\\ntr td {\\n  border-bottom: 1px solid lightgray;\\n}\\n\\ntr:nth-child(even){\\n  background-color: #f2f2f2;\\n}\\n\\ntr:hover {\\n  background-color: #ddd;\\n}\\n\\nth:last-child {\\n  border-top-right-radius: 10px;\\n}\\n\\nth:first-child {\\n  border-top-left-radius: 10px;\\n}\\n\\n.acceptBtn {\\n  background-color: orange;\\n  color: white;\\n}\\n\\n.bezorgdbtn {\\n  background-color: green;\\n  color: white;\\n}\\n\\n.tabeldiv {\\n  width: -webkit-max-content;\\n  width: -moz-max-content;\\n  width: max-content;\\n  display: -webkit-box;\\n  display: flex;\\n  margin: auto;\\n}\\n\\n.ophaalbtn {\\n  background-color: green;\\n  color: white;\\n}\\n\\n.deleteBbtn {\\n  background-color: #dc3545;\\n  color: white;\\n}\\n\\n.btnShape {\\n  padding: 10px;\\n  -webkit-box-flex: 1;\\n          flex: 1;\\n  border: none;\\n}\\n\\nnav .breadcrumb {\\n  display: block;\\n  width: 100%;\\n}\\n\\nli.breadcrumb-item {\\n  display: initial;\\n}\\n\\n.auth-container {\\n  float: right;